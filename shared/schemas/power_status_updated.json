{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "Power Status Updated Schema",
  "required": [
    "status",
    "timestamp",
    "version"
  ],
  "properties": {
    "status": {
      "$id": "#/properties/status",
      "type": "integer",
      "title": "Power Status Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "timestamp": {
      "$id": "#/properties/timestamp",
      "type": "string",
      "title": "The Timestamp Schema",
      "default": "",
      "examples": [
        "2020-02-18T01:42:50.434103+00:00"
      ],
      "format": "date-time",
    },
    "version": {
      "$id": "#/properties/version",
      "type": "string",
      "title": "The Version Schema",
      "default": "",
      "examples": [
        "0.1"
      ],
      "pattern": "^\d\.\d$"
    }
  }
}
